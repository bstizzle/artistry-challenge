<h1>New Joiner Form</h1>

<%= form_for @artist_instrument do |f| %>
    <%= f.label :artist_id %>
    <%= f.collection_select :artist_id, @artists, :id, :name %><br>

    <%= f.label :instrument_id %>
    <%= f.collection_select :instrument_id, @instruments, :id, :name %><br>

    <%= f.submit %>
<% end %>